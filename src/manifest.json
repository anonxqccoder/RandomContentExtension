{
    "name": "Random Content Button",
    "version": "1.0.2",
    "description": "Open random content to stay entertained!",
    "manifest_version": 3,
    "author": "funbox1111",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_title": "Random Content Button"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "unlimitedStorage",
        "contextMenus"
    ]
}